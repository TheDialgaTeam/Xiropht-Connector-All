<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Xiropht-Connector-All</AssemblyName>
    <RootNamespace>Xiropht_Connector_All</RootNamespace>
    <TargetFramework>netstandard2.0</TargetFramework>
    <OutputType>Library</OutputType>
    <PackageId>Xiropht_Connector_All</PackageId>
    <Version>1.0.5.5</Version>
    <Authors>Sam Segura</Authors>
    <Company>Xiropht</Company>
    <Product>Xiropht Connector All</Product>
    <Description>Connector used by all tools for Xiropht</Description>
    <PackageProjectUrl>https://github.com/TheDialgaTeam/Xiropht-Connector-All</PackageProjectUrl>
    <RepositoryUrl>https://github.com/TheDialgaTeam/Xiropht-Connector-All</RepositoryUrl>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <Platforms>AnyCPU;x64;x86</Platforms>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <DefineConstants>DEBUG</DefineConstants>
    <Optimize>false</Optimize>
    <DebugType>embedded</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <Optimize>true</Optimize>
    <DebugType>none</DebugType>
  </PropertyGroup>

</Project>